   <select id='getExportColumns' resultType="String">
   select STUFF((

SELECT ',' + QUOTENAME(convert(varchar, starttime, 120)) 
                    from (
		select distinct starttime
from scheduledEvent es
	join punch p on p.eventschedule = es.id
	join person pe on pe.people_id = p.campusId
where  splitByInstructor = 1
			and starttime &gt; #{startterm,jdbcType=DATE}
      <if test="endterm != null">
        and starttime &lt; #{endterm,jdbcType=DATE}
      </if>
	) a
                    group by starttime
                    order by starttime desc

            FOR XML PATH(''), TYPE
 ).value('.', 'NVARCHAR(MAX)') 
        ,1,1,'')
  </select>
  
  <select id='export' resultType="map">
select label, people_id, lastName, firstName, middleName, ${columns}
from 
(
select label, people_id, lastName, firstName, middleName, starttime, se.splitbyinstructor
from split_by_leader sl
	join person p on sl.personid = p.id
	join leaders l on l.id = sl.leaderId
	left join  punch pn on pn.campusId = p.people_id
	left join scheduledEvent se on se.id = pn.eventschedule
where splitbyinstructor = 1  
		and starttime &gt; #{startterm,jdbcType=DATE}
      <if test="endterm != null">
        and starttime &lt; #{endterm,jdbcType=DATE}
      </if>
) src
	pivot (
		count(starttime) 
		for starttime in ( ${columns} )
	) P
order by label
  </select>
  
  
  <select  id="selectActiveLeaders" resultType="map">
  with seatCounts as (
	select leaderId, count(*) as count
	from split_by_leader
	where endTerm > dbo.getCurrentDate()
	group by leaderId
)
select id, label, coalesce(seatCounts.count, 0) as count
from leaders
	left join seatCounts on seatCounts.leaderId = leaders.id
where leaders.active = 1 and coalesce(seatCounts.count, 0) &lt; #{maxSeatCount,jdbcType=INTEGER}
order by leaders.label
  </select>
  
    <select  id="selectIsSplitByMentor" resultType="Integer">
select case  when count(*) > 0 then 1 else 0 end
from scheduledEvent
where 1=1
	and splitByInstructor = 1
  </select>
  
      <select  id="selectSeatCount" resultType="Integer">
with pcount as (
select count(*) as count
from person
where currentStudent = 1
), 
lcount as (
select count(*) as count
from leaders 
where active = 1
)
select case when (cast(coalesce(properties.strvalue, 0) as int) > pcount.count/lcount.count+1) then cast(coalesce(properties.strvalue, 0) as int)
else pcount.count/lcount.count+1 end
from pcount cross join lcount left join properties on properties.keyValue = 'SEAT_COUNT'
  </select>
